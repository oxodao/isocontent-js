{
    "name": "react-isocontent-dom",
    "version": "0.1.0",
    "description": "Render Isocontent ASTs with react DOM",
    "main": "lib/index",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "repository": "https://github.com/un-zero-un/isocontent-js",
    "author": "Yohan Giarelli <yohan@un-zero-un.fr>",
    "license": "MIT",
    "peerDependencies": {
        "react": ">=16.8"
    },
    "private": false,
    "devDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "dependencies": {
        "isocontent": "^0.1.0",
        "react-isocontent": "^0.1.0"
    },
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "rimraf ./lib && tsc --watch --outDir esm --module es2015"
    }
}
