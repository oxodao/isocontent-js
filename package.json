{
    "name": "isocontent-packages",
    "private": true,
    "devDependencies": {
        "assert": "^2.0.0",
        "jest": "^24.8.0",
        "lerna": "^3.14.1",
        "ts-jest": "^24.0.2"
    },
    "dependencies": {
        "@types/jest": "^24.0.13",
        "typescript": "^3.4.5"
    },
    "scripts": {
